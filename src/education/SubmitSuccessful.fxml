<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="education.SubmitSuccessfulController">
   <children>
      <ImageView fitHeight="406.0" fitWidth="600.0" layoutY="-6.0">
         <image>
            <Image url="@Assets/signup.png" />
         </image>
      </ImageView>
      <Label fx:id="success" layoutX="34.0" layoutY="14.0" prefHeight="121.0" prefWidth="539.0" text="Congratulations! You have successfully signed up!" textFill="WHITE">
         <font>
            <Font name="Comic Sans MS" size="23.0" />
         </font>
      </Label>
      <Button fx:id="ok" layoutX="251.0" layoutY="226.0" mnemonicParsing="false" onAction="#okButtonPressed" prefHeight="45.0" prefWidth="105.0" stylesheets="@Red%20Button%20Highlight.css" text="Ok" textFill="WHITE">
         <font>
            <Font name="Comic Sans MS" size="28.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
