<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="education.QuestionScreenController">
   <children>
      <ImageView fitHeight="728.0" fitWidth="1080.0" layoutY="-8.0">
         <image>
            <Image url="@Assets/signup.png" />
         </image>
      </ImageView>
      <Label fx:id="questionLabel" layoutX="102.0" layoutY="213.0" prefHeight="72.0" prefWidth="879.0" style="-fx-background-color: #216968;" text="Empty Label" textFill="#fcfcfc">
         <font>
            <Font name="Constantia" size="25.0" />
         </font>
      </Label>
      <Button fx:id="choiceA" layoutX="298.0" layoutY="335.0" mnemonicParsing="false" onAction="#choiceAClicked" prefHeight="63.0" prefWidth="149.0" stylesheets="@Red%20Button%20Highlight.css" text="A" textFill="WHITE">
         <font>
            <Font name="Comic Sans MS" size="16.0" />
         </font>
      </Button>
      <Button fx:id="choiceB" layoutX="635.0" layoutY="335.0" mnemonicParsing="false" onAction="#choiceBClicked" prefHeight="63.0" prefWidth="139.0" stylesheets="@Red%20Button%20Highlight.css" text="B" textFill="WHITE">
         <font>
            <Font name="Comic Sans MS" size="16.0" />
         </font>
      </Button>
      <Button fx:id="choiceC" layoutX="298.0" layoutY="439.0" mnemonicParsing="false" onAction="#choiceCClicked" prefHeight="63.0" prefWidth="149.0" stylesheets="@Red%20Button%20Highlight.css" text="C" textFill="WHITE">
         <font>
            <Font name="Comic Sans MS" size="16.0" />
         </font>
      </Button>
      <Button fx:id="choiceD" layoutX="635.0" layoutY="439.0" mnemonicParsing="false" onAction="#choiceDClicked" prefHeight="63.0" prefWidth="139.0" stylesheets="@Red%20Button%20Highlight.css" text="D" textFill="WHITE">
         <font>
            <Font name="Comic Sans MS" size="16.0" />
         </font>
      </Button>
      <Button fx:id="backButton" layoutY="90.0" mnemonicParsing="false" onAction="#backButtonPressed" styleClass="button-backandhome" stylesheets="@SubjectScreenStyle.css">
         <graphic>
            <ImageView fitHeight="100.0" fitWidth="100.0">
               <image>
                  <Image url="@Assets/back1600.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="homeButton" layoutX="964.0" layoutY="90.0" mnemonicParsing="false" onAction="#homeButtonPressed" styleClass="button-backandhome" stylesheets="@SubjectScreenStyle.css">
         <graphic>
            <ImageView fitHeight="100.0" fitWidth="100.0">
               <image>
                  <Image url="@Assets/home.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Label fx:id="questionNumber" layoutX="437.0" layoutY="77.0" prefHeight="82.0" prefWidth="208.0" text="Questions" textFill="WHITE">
         <font>
            <Font name="Comic Sans MS" size="39.0" />
         </font>
      </Label>
      <Button fx:id="returnToUserProfile" layoutX="450.0" layoutY="643.0" mnemonicParsing="false" onAction="#returnToUserProfilePressed" prefHeight="63.0" prefWidth="191.0" stylesheets="@Red%20Button%20Highlight.css" text="Return to user profile" textFill="WHITE">
         <font>
            <Font name="Comic Sans MS" size="16.0" />
         </font>
      </Button>
      <Label fx:id="scoreLabel" layoutX="42.0" layoutY="213.0" prefHeight="413.0" prefWidth="997.0" style="-fx-background-color: #216968;" text="Score: " textFill="#fcfcfc" visible="false" wrapText="true">
         <font>
            <Font name="Constantia" size="25.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
